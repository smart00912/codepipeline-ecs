{
  "executionRoleArn": "arn:aws:iam::921283538843:role/AWSECSTaskServiceRole",
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "128",
  "memory": "256",
  "family": "nginx-ecs"
}